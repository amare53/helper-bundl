{
    "name": "amare53/helper-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "version": "1.0.0",
    "minimum-stability": "stable",
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "Tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "Amare53\\HelperBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "aime nzolo",
            "email": "aimengwe@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/orm": "2.9",
        "symfony/framework-bundle": "5.3.*",
        "knplabs/knp-paginator-bundle": "5.6",
        "phpdocumentor/reflection-docblock": "5.2",
        "symfony/property-access": "5.3.*",
        "symfony/serializer": "5.3.*",
        "symfony/validator": "6.0.x-dev",
        "symfony/password-hasher": "6.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev",
        "symfony/browser-kit": "6.0.x-dev",
        "symfony/css-selector": "6.0.x-dev",
        "symfony/phpunit-bridge": "6.0.x-dev"
    }
}
